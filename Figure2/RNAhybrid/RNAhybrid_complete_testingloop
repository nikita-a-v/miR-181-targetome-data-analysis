for fasta in $(ls) 
do

RNAhybrid -s 3utr_human -f 2,7 -u 3 -c -q $mirnaAnc -t $fasta -n 500 -b 1 > "$resdiranc3/$fasta-RNAhybrid_resutls_nc3.txt"

echo "created RNAhybrid result for $fasta"

done

echo "nc4"
mkdir $resdiranc4


for fasta in $(ls) 
do

RNAhybrid -s 3utr_human -f 2,7 -u 3 -c -q $mirnaAnc -t $fasta -n 500 -b 1 > "$resdiranc4/$fasta-RNAhybrid_resutls_c4.txt"

echo "created RNAhybrid result for $fasta"

done

